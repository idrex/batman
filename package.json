{
  "name": "batman",
  "version": "1.0.0",
  "description": "Node full stack application scaffolding",
  "author": "Drex",
  "private": true,
  "scripts": {
    "start": "cd server && npm run start",
    "build": "npm run clientbuild && npm run serverbuild",
    "lint": "npm run clientLint && npm run serverLint",
    "startclient": "cd client && npm run start",
    "clientInstall": "cd client && npm install --no-package-lock",
    "serverIntall": "cd server && npm install --no-package-lock",
    "postinstall": "npm run clientInstall && npm run serverIntall",
    "serverbuild": "cd server && npm run build",
    "clientbuild": "cd client && npm run build",
    "clientLint": "cd client && npm run lint",
    "serverLint": "cd server && npm run lint",
    "docker:build": "cd server && docker build -t node/batman .",
    "docker:run": "cd server && docker run -d --name batman -p 7001:7001 node/batman"
  },
  "keywords": [
    "node",
    "egg",
    "react",
    "ant-design-pro"
  ]
}
